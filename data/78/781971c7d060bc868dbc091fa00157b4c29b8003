</span></span><span style="display:flex;"><span>
</span></span><span style="display:flex;"><span>  pandigital.9 <span style="color:#333">&lt;-</span> <span style="color:#080;font-weight:bold">function</span>(x) <span style="color:#888"># Test if string is 9-pandigital</span>
</span></span><span style="display:flex;"><span>      (<span style="color:#06b;font-weight:bold">length</span>(x)<span style="color:#333">==</span><span style="color:#60e;font-weight:bold">9</span> <span style="color:#333">&amp;</span> <span style="color:#06b;font-weight:bold">sum</span>(<span style="color:#06b;font-weight:bold">duplicated</span>(x))<span style="color:#333">==</span><span style="color:#60e;font-weight:bold">0</span> <span style="color:#333">&amp;</span> <span style="color:#06b;font-weight:bold">sum</span>(x<span style="color:#333">==</span><span style="color:#60e;font-weight:bold">0</span>)<span style="color:#333">==</span><span style="color:#60e;font-weight:bold">0</span>)
</span></span><span style="display:flex;"><span>
</span></span><span style="display:flex;"><span>  <span style="color:#888">## Brute force</span>
</span></span><span style="display:flex;"><span>  pandigital.prod <span style="color:#333">&lt;-</span> <span style="color:#06b;font-weight:bold">vector</span>()
</span></span><span style="display:flex;"><span>  i <span style="color:#333">&lt;-</span> <span style="color:#60e;font-weight:bold">1</span>
</span></span><span style="display:flex;"><span>  <span style="color:#080;font-weight:bold">for</span> (m <span style="color:#080;font-weight:bold">in</span> <span style="color:#60e;font-weight:bold">2</span><span style="color:#333">:</span><span style="color:#60e;font-weight:bold">100</span>) {
</span></span><span style="display:flex;"><span>    <span style="color:#080;font-weight:bold">if</span> (m <span style="color:#333">&lt;</span> <span style="color:#60e;font-weight:bold">10</span>) 
</span></span><span style="display:flex;"><span>      n_start <span style="color:#333">&lt;-</span> <span style="color:#60e;font-weight:bold">1234</span> 
</span></span><span style="display:flex;"><span>    <span style="color:#080;font-weight:bold">else</span> 
</span></span><span style="display:flex;"><span>      n_start <span style="color:#333">&lt;-</span> <span style="color:#60e;font-weight:bold">123</span>
</span></span><span style="display:flex;"><span>    <span style="color:#080;font-weight:bold">for</span> (n <span style="color:#080;font-weight:bold">in</span> n_start<span style="color:#333">:</span><span style="color:#06b;font-weight:bold">round</span>(<span style="color:#60e;font-weight:bold">10000</span> <span style="color:#333">/</span> m)) {
</span></span><span style="display:flex;"><span>                                          <span style="color:#888"># List of digits</span>
</span></span><span style="display:flex;"><span>      digs <span style="color:#333">&lt;-</span> <span style="color:#06b;font-weight:bold">as.numeric</span>(<span style="color:#06b;font-weight:bold">unlist</span>(<span style="color:#06b;font-weight:bold">strsplit</span>(<span style="color:#06b;font-weight:bold">paste0</span>(m, n, m <span style="color:#333">*</span> n), <span style="background-color:#fff0f0">&#34;&#34;</span>)))
</span></span><span style="display:flex;"><span>                                          <span style="color:#888"># is Pandigital?</span>
</span></span><span style="display:flex;"><span>      <span style="color:#080;font-weight:bold">if</span> (<span style="color:#06b;font-weight:bold">pandigital.9</span>(digs)) {
</span></span><span style="display:flex;"><span>        pandigital.prod[i] <span style="color:#333">&lt;-</span> m <span style="color:#333">*</span> n
</span></span><span style="display:flex;"><span>        i <span style="color:#333">&lt;-</span> i <span style="color:#333">+</span> <span style="color:#60e;font-weight:bold">1</span>
</span></span><span style="display:flex;"><span>        <span style="color:#06b;font-weight:bold">print</span>(<span style="color:#06b;font-weight:bold">paste</span>(m, <span style="background-color:#fff0f0">&#34;*&#34;</span>, n, <span style="background-color:#fff0f0">&#34;=&#34;</span>, m <span style="color:#333">*</span> n))
</span></span><span style="display:flex;"><span>      }
</span></span><span style="display:flex;"><span>    }
</span></span><span style="display:flex;"><span>  }
</span></span><span style="display:flex;"><span>  answer <span style="color:#333">&lt;-</span> <span style="color:#06b;font-weight:bold">sum</span>(<span style="color:#06b;font-weight:bold">unique</span>(pandigital.prod))
</span></span><span style="display:flex;"><span>  <span style="color:#06b;font-weight:bold">print</span>(answer)</span></span></code></pre></div>
